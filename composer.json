{
  "name": "fourplayingchess/corlib",
  "description": "Provides core functionality for the web server.",
  "keywords": [],
  "require": {
    "php": ">=7.1",
    "illuminate/auth": "^8.12|^9.0|^10.0",
    "illuminate/container": "^8.12|^9.0|^10.0",
    "illuminate/contracts": "^8.12|^9.0|^10.0",
    "illuminate/database": "^8.12|^9.0|^10.0",
    "multicaret/laravel-acquaintances": "^3.5",
    "spatie/laravel-permission": "^5.11"
  },
  "require-dev": {
    "laravel/passport": "^11.0",
    "orchestra/testbench": "^6.23|^7.0|^8.0",
    "phpunit/phpunit": "^9.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Nicholas English",
      "email": "omatamix@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "FourPlayingChess\\CoreLib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "tests/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "FourPlayingChess\\CoreLib\\FourPlayingChessServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
