{
  "name": "fourplayingchess/corlib",
  "description": "Provides core functionality for the web server.",
  "keywords": [],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "laravel/passport": "^11.0",
    "orchestra/testbench": "^6.23|^7.0|^8.0",
    "phpunit/phpunit": "^9.4"
    "mockery/mockery": "1.0.x-dev",
    "fzaninotto/faker": "~1.4",
    "codeclimate/php-test-reporter": "^0.3.2",
    "doctrine/dbal": "^2.5"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Nicholas English",
      "email": "omatamix@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "FourPlayingChess\\CoreLib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "vendor/laravel/laravel/tests/TestCase.php"
    ],
    "files": [
      "tests/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "FourPlayingChess\\CoreLib\\FourPlayingChessServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
