{
  "name": "fourplayingchess/corlib",
  "description": "Provides core functionality for the web server.",
  "keywords": [],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "laravel/passport": "^11.0",
    "orchestra/testbench": "^6.23|^7.0|^8.0",
    "phpunit/phpunit": "^9.4"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Nicholas English",
      "email": "omatamix@gmail.com"
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "FourPlayingChess\\CoreLib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "tests/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev-main",
      "dev-master": "1.x-dev-master"
    },
    "laravel": {
      "providers": [
        "FourPlayingChess\\CoreLib\\FourPlayingChessServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
